<h1 class="center page-header">Articles</h1>


<div class="row">
  {{#each model as |article|}}
  {{#link-to 'article' article.id}}<h3>{{article.title}}</h3>
  <h5>By: {{article.author}}</h5>
  <p>{{article.teaser}}</p>
  {{/link-to}}
  {{/each}}
</div>



